<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.neda.carwarehouse">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CarWarehouse">
        <provider
            android:name=".provider.ContentProviderWithRoom"
            android:authorities="ContentProviderWithRoom"
            android:enabled="true"
            android:exported="true"></provider>
        <provider
            android:name=".provider.ContentProviderWithHelper"
            android:authorities="ContentProviderWithHelper"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".RecyclerViewActivity" />
        <activity android:name=".NavDrawerActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>