<?xml version="1.0" encoding="utf-8"?>
<com.neda.carwarehouse.layout.MyConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraint_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GestureActivity">

    <com.neda.carwarehouse.layout.MyFrameLayout
        android:id="@+id/frame_layout_id"
        android:layout_width="209dp"
        android:layout_height="329dp"
        android:background="#4AE61F"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <com.neda.carwarehouse.layout.MyLinearLayout
            android:background="#FFEB3B"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:orientation="vertical">

            <com.neda.carwarehouse.layout.MyTextView
                android:id="@+id/textView"
                android:background="#FF9800"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView" />

        </com.neda.carwarehouse.layout.MyLinearLayout>
    </com.neda.carwarehouse.layout.MyFrameLayout>

</com.neda.carwarehouse.layout.MyConstraintLayout>